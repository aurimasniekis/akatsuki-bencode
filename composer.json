{
    "name": "akatsuki/bencode",
    "type": "library",
    "description": "Akatsuki Bencode Component",
    "keywords": ["bencode", "akatsuki"],
    "homepage": "https://akatsuki.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Aurimas Niekis",
            "email": "aurimas@niekis.lt"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.1"
    },
    "autoload": {
        "psr-4": { "Akatsuki\\Component\\Bencode\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "Akatsuki\\Component\\Bencode\\Test\\": "tests" }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-ci": "vendor/bin/phpunit -c phpunit.xml.ci"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
